{
    "name": "@vitkuz/aws-opensearch-adapter",
    "version": "1.1.0",
    "description": "Functional OpenSearch adapter for AWS",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "pub": "npm publish --access public",
        "build": "tsup",
        "dev": "tsup --watch",
        "prepublishOnly": "npm run format && npm run build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
    },
    "dependencies": {
        "@aws-sdk/credential-provider-node": "^3.962.0",
        "@opensearch-project/opensearch": "^3.5.1",
        "dotenv": "^17.2.3",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/uuid": "^10.0.0",
        "prettier": "^3.7.4",
        "rimraf": "^6.0.0",
        "tsup": "^8.0.0",
        "typescript": "^5.0.0"
    },
    "author": "Vitali Kuzmenka",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/vitkuz/vitkuz-aws-opensearch-adapter.git"
    },
    "bugs": {
        "url": "https://github.com/vitkuz/vitkuz-aws-opensearch-adapter/issues"
    },
    "homepage": "https://github.com/vitkuz/vitkuz-aws-opensearch-adapter#readme"
}
